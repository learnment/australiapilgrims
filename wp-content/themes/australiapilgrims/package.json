{
  "name": "australiapilgrims",
  "version": "1.0.0",
  "description": "Australia Pilgrims WordPress Theme",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "build:blocks && webpack --mode production",
    "watch": "webpack --mode production --watch",
    "build:blocks": "wp-scripts build blocks/hero-banner/index.js --output-path=blocks/hero-banner",
    "start:blocks": "wp-scripts start blocks/hero-banner/index.js --output-path=blocks/hero-banner"
  },
  "keywords": [
    "wordpress",
    "theme",
    "gutenberg"
  ],
  "author": "Your Name",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "@wordpress/block-editor": "^12.0.0",
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^25.0.0",
    "@wordpress/element": "^5.0.0",
    "@wordpress/i18n": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@wordpress/scripts": "^30.26.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
